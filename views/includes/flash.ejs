<% if(success && success.length){ %>
<div class="alert mt-2 alert-success alert-dismissible fade show flash-message" role="alert">
    <%= success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
<% if(error && error.length){ %>
    <div class="alert mt-2 alert-danger alert-dismissible fade show flash-message" role="alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>
